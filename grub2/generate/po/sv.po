#: generate:95
msgid "Boot to runlevel '3' (text mode)"
msgstr "Starta till körnivå '3' (text läge)"

#: generate:96
msgid "Boot to runlevel '4' (graphical login) (default)"
msgstr "Starta till körnivå '4' (grafisk inloggning) (standard)"

#: generate:97
msgid "Enable 'Copy everything to RAM'"
msgstr "Aktivera 'Kopiera allt till RAM'"

#: generate:98
msgid "Disable 'Copy everything to RAM' (default)"
msgstr "Avaktivera 'Kopiera allt till RAM' (standard)"

#: generate:99
msgid "Enable numlock on boot"
msgstr "Aktivera numlock vid uppstart"

#: generate:100
msgid "Disable numlock on boot (default)"
msgstr "Avaktivera numlock vid uppstart (standard)"

#: generate:101
