msgstr "Aktivera ibus"

#: generate:102
msgid "Disable Ibus (default)"
msgstr "Inaktivera ibus (standard)"

#: generate:103
msgid "Enable synaptics fdi (sometimes unreliable)"
msgstr "Aktivera synaptics FDI (ibland opålitlig)"

#: generate:104
msgid "Disable synaptics fdi (reliable) (default)"
msgstr "Inaktivera synaptics FDI (tillförlitlig) (standard)"

#: generate:105
msgid "No sound on boot"
msgstr "Inget ljud vid uppstart"

#: generate:106
msgid "Enable sound on boot (default)"
msgstr "Aktivera ljud vid start (standard)"

#: generate:107
msgid "Enable autologin on boot (default)"
