#: generate:89
msgid "Start _DISTRONAME_"
msgstr "Iniciar _DISTRONAME_"

#: generate:90
msgid "Memory Test"
msgstr "Testar a memória"

#: generate:91
msgid "Change language"
msgstr "Mudar o idioma"

#: generate:92
msgid "Change keyboard layout"
msgstr "Alterar configuração do teclado"

#: generate:93
msgid "Change time zone"
msgstr ""

#: generate:94
msgid "Change boot options"
msgstr "Alterar opções de boot"

#: generate:95
msgid "Boot to runlevel '3' (text mode)"
msgstr "Iniciar para nivel '3' (modo texto)"

#: generate:96
msgid "Boot to runlevel '4' (graphical login) (default)"
msgstr "Iniciar para nivel '4' (login gráfico) (predefinido)"

#: generate:97
msgid "Enable 'Copy everything to RAM'"
msgstr "Habilitar 'Copiar tudo para RAM'"

#: generate:98
msgid "Disable 'Copy everything to RAM' (default)"
msgstr "Deshabilitar 'Copiar tudo para RAM' (predefinido)"

#: generate:99
msgid "Enable numlock on boot"
msgstr "Habilitar numlock ao iniciar"

#: generate:100
msgid "Disable numlock on boot (default)"
msgstr "Deshabilitar numlock ao iniciar (predefinido)"

#: generate:101
msgid "Enable Ibus"
msgstr "Ativar Ibus"

#: generate:102
msgid "Disable Ibus (default)"
msgstr "Desativar Ibus (padrão)"

#: generate:103
msgid "Enable synaptics fdi (sometimes unreliable)"
msgstr "Habilitar synaptics fdi (às vezes pouco fiável)"

#: generate:104
msgid "Disable synaptics fdi (reliable) (default)"
msgstr "Deshabilitar synaptics fdi (fiável) (predefinido)"

#: generate:105
msgid "No sound on boot"
msgstr "Sem som ao iniciar"

#: generate:106
msgid "Enable sound on boot (default)"
msgstr "Ativar som ao iniciar (padrão)"

#: generate:107
msgid "Enable autologin on boot (default)"
msgstr "Ativar autologin ao iniciar (padrão)"

#: generate:108
msgid "Disable autologin on boot"
msgstr "Desativar autologin ao iniciar"

#: generate:109
msgid "Enable gfxpayload=keep (default)"
msgstr "Ativar gfxpayload=keep (padrão)"

#: generate:110
msgid "Disable gfxpayload=keep (if it doesn't load try this)"
msgstr "Desativar gfxpayload=keep (se ele não carrega, tente isso)"

#: generate:111
msgid "Change root password"
msgstr "Mudar a senha do root"

#: generate:112
msgid "New root password (choose 'ask' to be asked at boot):"
msgstr ""
"Nova senha do root (escolha 'ask' para que o sistema peça a senh ao iniciar):"

#: generate:113
msgid "Set a proxy"
msgstr "Definir um proxy"

#: generate:114
msgid "New proxy (empty for no proxy):"
msgstr "Novo proxy (deixe vazio para não usar proxy):"

#: generate:115
msgid "Boot from an ISO"
msgstr "Iniciar de uma ISO"

#: generate:116
msgid "Path to the ISO to boot from:"
msgstr "Caminho para a ISO da qual iniciar:"

#: generate:117
msgid "Live system is loading..."
msgstr "Sistema live está carregando..."

#: generate:118
msgid "Return to boot menu"
msgstr "Voltar para menu inicial"

#: generate:119
msgid "Save all settings to be autoloaded for future boots"
msgstr ""
"Salvar todas as configurações para serem auto carregadas em inicializações "
