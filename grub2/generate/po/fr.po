#, sh-format
msgid "y"
msgstr "o"

#, sh-format
msgid "Y"
msgstr "O"

#, sh-format
msgid "n"
msgstr "n"

#, sh-format
msgid "N"
msgstr "N"

#, sh-format
msgid "Change keyboard layout"
msgstr "Changer la disposition du clavier"

#, sh-format
msgid "Start _DISTRONAME_"
msgstr "Démarrer _DISTRONAME_"

#, sh-format
msgid "Live system is loading..."
msgstr "Le système Live démarre..."

#, sh-format
msgid "Change boot options"
msgstr "Changer les options de démarrage"

#, sh-format
msgid "Memory Test"
msgstr "Tester la mémoire (RAM)"

#, sh-format
msgid "Change language"
msgstr "Changer la langue"

#, sh-format
msgid "Change time zone"
msgstr "Changer le fuseau horaire"

#, sh-format
msgid "Save all settings to be autoloaded for future boots"
msgstr "Enregistrer les paramètres pour les futurs démarrages"

#, sh-format
msgid "Do you want to skip the language selection menu? [y/N]"
msgstr "Voulez-vous ignorer la sélection de langue ? [o/N]"

#, sh-format
msgid "Invalid response"
msgstr "Réponse non valide"

#, sh-format
msgid ""
"There was an error saving your settings. Are the live files located on a "
"writeable media like an USB key (i.e. not a CDROM)?"
msgstr ""
"Il y a eu une erreur d'enregistrer de vos paramètres. Les fichiers se "
"trouvent-ils sur un support inscriptible comme une clé USB (c'est à dire pas "
"un CDROM) ?"

#, sh-format
msgid "Return to boot menu"
msgstr "Retourner au menu de démarrage"

#, sh-format
msgid "Boot to runlevel '3' (text mode)"
msgstr "Démarrer en runlevel '3' (mode texte)"

#, sh-format
msgid "Boot to runlevel '4' (graphical login) (default)"
msgstr "Démarrer en runlevel '4' (mode graphique) (défaut)"

#, sh-format
msgid "Enable 'Copy everything to RAM'"
msgstr "Activer la 'Copie intégrale vers la RAM'"

#, sh-format
msgid "Disable 'Copy everything to RAM' (default)"
msgstr "Désactiver la 'Copie intégrale vers la RAM' (défaut)"

#, sh-format
msgid "Enable numlock on boot"
msgstr "Activer le verrou numérique au démarrage"

#, sh-format
msgid "Disable numlock on boot (default)"
msgstr "Désactiver le verrou numérique au démarrage (défaut)"

#, sh-format
msgid "Enable Ibus"
msgstr "Activer Ibus"

#, sh-format
msgid "Disable Ibus (default)"
msgstr "Désactiver Ibus (par défaut)"

#, sh-format
msgid "Enable synaptics fdi (sometimes unreliable)"
msgstr "Activer le pavé tactile Synaptic (parfois non fiable)"

#, sh-format
msgid "Disable synaptics fdi (reliable) (default)"
msgstr "Désactiver le pavé tactile Synaptic (fiable) (défaut)"

#, sh-format
msgid "No sound on boot"
msgstr "Pas de son au démarrage"

#, sh-format
msgid "Enable sound on boot (default)"
msgstr "Activer le son au démarrage (par défaut)"

#, sh-format
msgid "Enable autologin on boot (default)"
msgstr "Activer la connexion automatique (par défaut)"

#, sh-format
msgid "Disable autologin on boot"
msgstr "Désactiver la connexion automatique"

#, sh-format
msgid "Enable gfxpayload=keep (default)"
msgstr "Activer gfxpayload=keep (par défaut)"

#, sh-format
msgid "Disable gfxpayload=keep (if it doesn't load try this)"
msgstr "Désactiver gfxpayload=keep (essayez ça si problème)"

#, sh-format
msgid "Change root password"
msgstr "Changer mot de passe root"

#, sh-format
msgid "New root password (choose 'ask' to be asked at boot):"
msgstr "Mot de passe root (« ask » pour le taper plus tard) :"

#, sh-format
msgid "Set a proxy"
msgstr "Définissez un proxy"

#, sh-format
msgid "New proxy (empty for no proxy):"
msgstr "Nouveau proxy (vide pour aucun proxy) :"

#, sh-format
msgid "Boot from an ISO"
msgstr "Démarrer à partir d'un ISO"

#, sh-format
msgid "Path to the ISO to boot from:"
msgstr "Chemin vers l'ISO depuis lequel démarrer :"

#, sh-format
msgid "Custom linux kernel parameters"
msgstr "Paramètres personnalisés de démarrage du noyau Linux"

#, sh-format
msgid "Enter custom linux kernel parameters:"
msgstr "Paramètres personnalisés de démarrage de Linux :"
